{
  "name": "rdf-object-proxy",
  "version": "1.0.0",
  "description": "A proxy for the rdf-object library to match the LDflex API ",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint lib/* __tests__/* --ext .ts",
    "lint:fix": "eslint lib/* __tests__/* --ext .ts --fix",
    "build": "tsc",
    "prepare": "tsc",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeswr/rdf-object-proxy.git"
  },
  "keywords": [
    "LDflex",
    "RDF",
    "object"
  ],
  "author": "Jesse Wright <jesse.wright@anu.edu.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeswr/rdf-object-proxy/issues"
  },
  "homepage": "https://github.com/jeswr/rdf-object-proxy#readme",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^27.0.1",
    "pre-commit": "^1.2.2",
    "rdf-js": "^4.0.2",
    "ts-jest": "^27.0.1",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "coverageReporters": [
      "json-summary", 
      "text",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ],
  "release": {
    "branches": [
      "main",
      "+([0-9])?(.{+([0-9]),x}).x",
      "next",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "dependencies": {
    "@rdfjs/data-model": "^1.2.0",
    "rdf-object": "^1.8.0"
  }
}
